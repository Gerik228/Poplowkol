declare
  l_clob clob;
begin
  select des_test
    into l_clob
    from bi_portal_faq
   where id = to_number(apex_application.g_x01);

  apex_util.print_clob(l_clob);

exception
  when no_data_found then
    sys.htp.prn('Контент не найден');
  when others then
    sys.htp.prn('Ошибка: ' || sqlerrm);
end;