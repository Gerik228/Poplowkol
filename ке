    padding: 0 24px 24px;
    display: flex;
    gap: 12px;
}

.adaptive-btn {
    flex: 1;
    padding: 12px 16px;
    border-radius: 10px;
    font-size: 14px;
    font-weight: 500;
    cursor: pointer;
    transition: all 0.2s ease;
    border: none;
}

.adaptive-btn.primary {
    background: #3b82f6;
    color: white;
}

