    position: relative;
    z-index: 1;
}

.gradient-icon {
    font-size: 48px;
    margin-bottom: 20px;
    text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
}

.gradient-title {
    font-size: 24px;
    font-weight: 700;
    margin: 0 0 16px 0;
